{
  "name": "@skogkalk/common",
  "version": "1.0.0",
  "description": "",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.js",
      "default": "./dist/index.js"
    },
    "./dist/src/parseTree": {
      "types": "./dist/src/parseTree/index.d.ts",
      "import": "./dist/src/parseTree/index.js",
      "require": "./dist/src/parseTree/index.js",
      "default": "./dist/src/parseTree/index.js"
    },
    "./dist/src/parseTree/*": {
      "types": "./dist/src/parseTree/*.d.ts",
      "import": "./dist/src/parseTree/*.js",
      "require": "./dist/src/parseTree/*.js",
      "default": "./dist/src/parseTree/*.js"
    },
    "./dist/src/config/*": {
      "types": "./dist/src/config/*.d.ts",
      "import": "./dist/src/config/*.js",
      "require": "./dist/src/config/*.js",
      "default": "./dist/src/config/*.js"
    },
    "./dist/src/types/*": {
      "types": "./dist/src/types/*.d.ts",
      "import": "./dist/src/types/*.js",
      "require": "./dist/src/types/*.js",
      "default": "./dist/src/types/*.js"
    },
    "./dist/src/visual/*": {
      "types": "./dist/src/visual/*.d.ts",
      "import": "./dist/src/visual/*.js",
      "require": "./dist/src/visual/*.js",
      "default": "./dist/src/visual/*.js"
    },
    "./dist/src/util/*": {
      "types": "./dist/src/util/*.d.ts",
      "import": "./dist/src/util/*.js",
      "require": "./dist/src/util/*.js",
      "default": "./dist/src/util/*.js"
    }
  },
  "scripts": {
    "start": "echo \"Error: no start specified\"",
    "build": "tsc",
    "watch": "tsc -b -w",
    "test": "vitest run"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "jsdom": "^24.0.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "typescript": "^5.3.3",
    "vitest": "^4.0.14"
  },
  "dependencies": {
    "apexcharts": "^3.45.0",
    "dompurify": "^3.0.0",
    "react-apexcharts": "^1.4.1",
    "react-bootstrap": "^2.10.0",
    "react-icons": "^4.12.0"
  },
  "peerDependencies": {
    "@types/react": "^18.0.0 || ^19.0.0",
    "@types/react-dom": "^18.0.0 || ^19.0.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  }
}
